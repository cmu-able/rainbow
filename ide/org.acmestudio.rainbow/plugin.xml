<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="org.acmestudio.rainbow.commands.category"
            name="Sample Category">
      </category>
      <command
            categoryId="org.acmestudio.rainbow.commands.category"
            description="Connects to a running Rainbow instance"
            id="org.acmestudio.rainbow.commands.connect"
            name="Connect...">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.acmestudio.rainbow.handlers.RainbowConnectionHandler"
            commandId="org.acmestudio.rainbow.commands.connect">
      </handler>
      <handler
            class="org.acmestudio.rainbow.handlers.RainbowDisconnectionHandler"
            commandId="org.acmestudio.rainbow.commands.disconnect">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.acmestudio.rainbow.commands.connect"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+6">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="org.acmestudio.rainbow.menus.rainbow"
               label="Rainbow"
               mnemonic="R">
            <command
                  commandId="org.acmestudio.rainbow.commands.connect"
                  id="org.acmestudio.rainbow.menus.rainbow"
                  mnemonic="S">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.acmestudio.ui.property.tabbed.propertyTabs">
      <propertyTabs
            contributorId="org.acmestudio.ui.MultiPageEditor">
         <propertyTab
               category="acmeelements"
               id="org.acmestudio.rainbow.gauges"
               indented="true"
               indentedUnder="org.acmestudio.ui.view.property.properties"
               label="Rainbow">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.acmestudio.ui.property.tabbed.propertySections">
      <propertySections
            contributorId="org.acmestudio.ui.MultiPageEditor">
         <propertySection
               class="org.acmestudio.rainbow.ui.part.GaugesSectionAdapter"
               filter="org.acmestudio.rainbow.ui.part.GaugesSection$Filter"
               id="org.acmestudio.rainbow.gauges.section"
               tab="org.acmestudio.rainbow.gauges">
         </propertySection>
      </propertySections>
   </extension>

</plugin>
